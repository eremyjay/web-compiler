  "name": "@eremyjay/web-compiler",
  "version": "1.0.0",
  "description": "Combination of different javascript, html and css processing tools all in one to do javascript styled compilation",
  "main": "compile.js",
  "homepage": "https://github.com/eremyjay/web-compiler",
  "keywords": [
    "web",
    "compiler"
  ],
  "dependencies": {
    "clean-css": "^4.2.3",
    "fs-extra": "^9.0.1",
    "html-minifier": "^4.0.0",
    "javascript-obfuscator": "^2.9.5",
    "jquery": "^3.5.1",
    "munch": "git+https://github.com/interactdo/munchjs.git",
    "ncp": "^2.0.0",
    "terser": "^5.5.1"
  },
  "devDependencies": {},
  "resolutions": {},
  "scripts": {
    "build": "npm build",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "start": "node src/",
    "preinstall": "# npx npm-force-resolutions",
    "mocha": "mocha test/ --recursive"
  },
  "repository": {
    "type": "git",
